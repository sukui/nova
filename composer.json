{
  "name": "zanphp/nova",
  "description": "nova",
  "repositories": {
    "0": {
      "type": "git",
      "url": "https://github.com/sukui/nova-foundation.git"
    },
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  },
  "require": {
    "zanphp/exception": "dev-master",
    "zanphp/contracts": "dev-master",
    "zanphp/container": "dev-laogui",
    "zanphp/nova-foundation": "dev-laogui",
    "zanphp/thrift-serialization": "dev-laogui"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "Kdt\\Iron\\Nova\\": "src",
      "Kdt\\Iron\\Nova\\Tests\\": "tests"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Com\\Youzan\\Sso\\": "tests/resource/sso/sdk/gen-php",
      "Com\\Youzan\\Material\\": "tests/resource/material/sdk/gen-php"
    }
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "minimum-stability": "dev",
  "prefer-stable": true

}
